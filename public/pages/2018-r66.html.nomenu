<script>
function initHQDesign() {
    
    // --- 1. PRODUCT PAGE LOGIC ---
    if(document.querySelector('.ProductItem')) {
        document.querySelectorAll('.sqs-add-to-cart-button-inner').forEach(btn => btn.innerText = "Enquire About Aircraft");

        const gallery = document.querySelector('.ProductItem-gallery');
        const slider = document.querySelector('.ProductItem-gallery-slides');
        const slides = document.querySelectorAll('.ProductItem-gallery-slides-item');
        
        if (gallery && slider && slides.length > 1 && !document.querySelector('.hq-carousel-arrow')) {
            const leftBtn = document.createElement('div'); leftBtn.className = 'hq-carousel-arrow hq-arrow-left';
            const rightBtn = document.createElement('div'); rightBtn.className = 'hq-carousel-arrow hq-arrow-right';
            gallery.appendChild(leftBtn); gallery.appendChild(rightBtn);

            let currentIndex = 0;
            const updateCarousel = () => {
                slider.style.transform = `translateX(-${currentIndex * 100}%)`;
                leftBtn.style.opacity = currentIndex === 0 ? '0.3' : '1';
                leftBtn.style.pointerEvents = currentIndex === 0 ? 'none' : 'auto';
                rightBtn.style.opacity = currentIndex === slides.length - 1 ? '0.3' : '1';
                rightBtn.style.pointerEvents = currentIndex === slides.length - 1 ? 'none' : 'auto';
            };
            leftBtn.addEventListener('click', () => { if(currentIndex > 0) { currentIndex--; updateCarousel(); }});
            rightBtn.addEventListener('click', () => { if(currentIndex < slides.length - 1) { currentIndex++; updateCarousel(); }});
            updateCarousel();
        }

        const containers = document.querySelectorAll('.ProductItem-details-excerpt, .ProductItem-details-description');
        containers.forEach(container => {
            if(container.classList.contains('hq-processed')) return;
            const paragraphs = Array.from(container.querySelectorAll('p'));
            if(paragraphs.length === 0) return; 

            const descHeader = document.createElement('div'); descHeader.className = 'hq-section-header'; descHeader.innerText = 'DESCRIPTION';
            const descBlock = document.createElement('div'); descBlock.className = 'hq-description-block';
            const specHeader = document.createElement('div'); specHeader.className = 'hq-section-header'; specHeader.innerText = 'EQUIPMENT & FEATURES';
            const specGrid = document.createElement('div'); specGrid.className = 'hq-spec-grid';

            let isSpecSection = false;
            let hasSpecItems = false;

            paragraphs.forEach(p => {
                const text = p.innerText.trim();
                if(!text || text === "Description" || text === "Additional Options") return; 
                if (text.includes("following equipment") || text.includes("Additional Options")) { isSpecSection = true; return; }

                if (isSpecSection) {
                    const item = document.createElement('div'); item.className = 'hq-spec-item'; item.innerText = text;
                    specGrid.appendChild(item);
                    hasSpecItems = true;
                } else {
                    const para = document.createElement('p'); para.innerText = text;
                    descBlock.appendChild(para);
                }
            });

            const wrapper = document.createElement('div');
            wrapper.className = 'hq-new-ui-wrapper';
            if (descBlock.hasChildNodes()) { wrapper.appendChild(descHeader); wrapper.appendChild(descBlock); }
            if (hasSpecItems) { wrapper.appendChild(specHeader); wrapper.appendChild(specGrid); }
            
            if(wrapper.hasChildNodes()) {
                container.appendChild(wrapper);
                paragraphs.forEach(p => p.classList.add('hq-hidden-original'));
                container.classList.add('hq-processed');
            }
        });
    }

    // --- 2. LISTINGS & GENERAL ---
    // (Note: Click listener removed to make listings static)

    document.querySelectorAll('h1, h2, h3').forEach(header => {
        if (['currently','listings','sold','inventory'].some(k => header.textContent.toLowerCase().includes(k))) {
            header.classList.add('aviation-header-line');
        }
    });
    const observer = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
            if (entry.isIntersecting) {
                entry.target.classList.add('visible');
                if(entry.target.classList.contains('summary-item')) {
                    entry.target.style.opacity = "1"; entry.target.style.transform = "translateY(0)";
                }
                observer.unobserve(entry.target);
            }
        });
    }, { threshold: 0.1 });
    document.querySelectorAll('.summary-item, .aviation-header-line, .aviation-badge-container').forEach(el => observer.observe(el));
}
document.addEventListener("DOMContentLoaded", initHQDesign);
window.addEventListener('mercury:load', initHQDesign);
</script><script>Static.COOKIE_BANNER_CAPABLE = true;</script>
